# ===============================================
# SUPABASE CONFIGURATION
# ===============================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# ===============================================
# ENCRYPTION CONFIGURATION
# ===============================================
# Must be the same across all repos (Repo 1, 2, 3)
ENCRYPTION_KEY=your-32-character-encryption-key-here

# ===============================================
# TELEGRAM API CONFIGURATION (Telethon)
# ===============================================
# Get these from https://my.telegram.org/apps
TG_API_ID=12345678
TG_API_HASH=your_telegram_api_hash_here

# Target chat to monitor for trading signals
# Format: -1001234567890 (with minus sign for supergroups)
TARGET_CHAT_ID=-1001234567890

# ===============================================
# ADMIN CONFIGURATION
# ===============================================
# Your Telegram user ID for receiving logs/notifications
ADMIN_USER_ID=123456789

# ===============================================
# AUTOTRADER RULES (Optional - configured in code)
# ===============================================
# MIN_CONFIDENCE=95.0
# TRADE_AMOUNT=1.0
# TRADE_DURATION=60
